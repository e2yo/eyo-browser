    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-plugin-react": "^7.9.1",
    "eyo-kernel": "^2.3.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.5",
    "preact": "^8.2.9",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-md5-hash": "0.0.6"
  },
  "scripts": {
    "test": "eslint .",
    "clear": "rm -rf ./dist",
    "dict": "mkdir ./dist && cp ./node_modules/eyo-kernel/dict/*.txt ./dist",
    "before-build": "npm run clear && npm run dict",
    "build:dev": "npm run before-build && webpack --mode=development",
    "build:prod": "npm run before-build && webpack --mode=production"
  }
